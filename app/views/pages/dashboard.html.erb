<div class="row">
  <div class="col-md-2">
  <h3><%= @user.username %></h3>

  </div>
  <div class="col-md-5">
    <h3>Foods</h3>
    <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th>Created</th>
        </tr>
      </thead>
      <tbody>
        <% @foods.each do |food| %>
        <tr>
          <td><%= link_to food.name, food %></td>
          <td><%= time_ago_in_words(food.created_at) %> ago</td>

        </tr>
        <% end %>
      </tbody>
    </table>
      <h4><%= link_to "Gallery", foods_path %></h4>
      <h4><%= link_to "Add Food", new_food_path %></h4>
      <h4><%= link_to "Categories", categories_path %></h4>
  </div>
</div>